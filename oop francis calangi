  class Vehicle{
      string brand;
      int fuel;
        Vehicle (String brand,int fuel){
          this.brand= brand;
          this.fuel= fuel;
        }
    }
  class Car extends Vehicle {
        int doors;
    Car (String bramd,int fuel,int doors){
           super(brand,fuel);
           this.doors= doors;
   }
  void drive(){
    if fuel > 0 {
     fuel --;
   System.out.println(brand +" is driving. Fuel left:"+ fuel);
   }else{
     System.out.println("outoffuel!");
   }
 }
}
   public class Main2{
     public static void main(String[]args){
      Car c= newCar("Toyota",5,4);
      c.drive();
  }
}

 class BankAccount{
   private double balance;
   public void deposit(double amount){
      balance += amount;
}
  public void withdraw(double amount){
     if (amount <= balance) {
         balance -= amount;
        }else{
           System.out.println("Insufficient balance!");
        }
   } 
       public doublegetBalance(){
            return balance;
   }
      public class Main3{
           public static void main(String[]args){
           BankAccount account= new BankAccount();
           account.deposit (500);
           account.withdraw (200);
           System.out.lrintln("Balance:"+account.getBalance());
    }
 }

   class Person{
        private int age;
        public int getAge(){
             return age;
  }
   public void setAge(int age){
        if (age > 0){
           this.age= age;
       }else{
        System.out.println("Age must be positive.");
       }
     }
 } 
   public class Main4{
     public static void main(String[]args){
     Person p= new Person();
       p.setAge (20);
       System.out.println("Age:"+p.getAge());
  }
}

